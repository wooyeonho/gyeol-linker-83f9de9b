{
  "agent": {
    "name": "OpenClaw Living Agent",
    "description": "A proactive, living AI assistant.",
    "systemPromptFile": "/app/system_prompt.md",
    "model": {
      "provider": "openai",
      "model": "gpt-4-turbo",
      "temperature": 0.7
    }
  },
  "server": {
    "port": 18789
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "${TELEGRAM_BOT_TOKEN}" 
    },
    "discord": {
      "enabled": false,
      "token": "${DISCORD_BOT_TOKEN}"
    }
  },
  "sandbox": {
    "enabled": true,
    "workDir": "/app/data/workspace"
  },
  "skills": [
    "web-search",
    "file-ops",
    "shell"
  ]
}
